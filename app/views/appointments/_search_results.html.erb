<!-- app/views/appointments/_search_results.html.erb -->
<% if @results.any? %>
  <h2>Search Results:</h2>
  <ul>
    <% @results.each do |appointment| %>
        <%= render appointment %>
        <!-- Add other appointment details you want to display -->
      
    <% end %>
  </ul>
<% else %>
  <p>No results found.</p>
<% end %>
